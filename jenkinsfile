pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'venkat', url: 'https://github.com/venkatw3g/venkatw3gnewrepo.git']])
            }
        }
        stage ('build') {
            steps {
                echo "maven clean install"
            }
        }
        stage ('gitclone'){
            steps {
                git credentialsId: 'venkat', url: 'https://github.com/venkatw3g/venkatw3gnewrepo.git'
            }
        }
    }
}
